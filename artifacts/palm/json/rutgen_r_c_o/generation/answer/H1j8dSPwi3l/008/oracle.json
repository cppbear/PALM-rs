          "        fn next(&mut self) -> Result<Option<u8>> {",
          "            if self.index < self.data.len() {",
          "                let byte = self.data[self.index];",
          "                self.index += 1;",
          "                Ok(Some(byte))",
          "            } else {",
          "                Ok(None)",
          "            }",
