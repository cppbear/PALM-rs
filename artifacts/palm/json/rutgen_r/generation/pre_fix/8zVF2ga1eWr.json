                  "{",
                  "    // Test input where ch is not a double quote",
                  "    assert_eq!(is_escape(b'a', false), false);",
                  "    assert_eq!(is_escape(b'a', true), false);",
                  "    ",
                  "    // Test input where ch is the escape character",
                  "    assert_eq!(is_escape(b'\\\\', false), true);",
