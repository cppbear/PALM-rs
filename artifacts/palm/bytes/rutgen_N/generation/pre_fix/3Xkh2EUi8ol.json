                  "    use std::sync::atomic::{AtomicPtr, Ordering};",
                  "    use std::ptr;",
                  "    ",
                  "    struct Bytes {",
                  "        ptr: *const u8,",
                  "        len: usize,",
                  "        data: AtomicPtr<()>,",
                  "        vtable: &'static VTable,",
                  "    }",
                  "    ",
                  "    struct OwnedLifetime {",
                  "        ref_cnt: AtomicUsize,",
                  "    }",
