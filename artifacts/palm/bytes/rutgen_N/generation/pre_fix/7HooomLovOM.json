            "",
            "impl BufMut {",
            "    fn new() -> Self {",
            "        BufMut { data: Vec::new() }",
            "    }",
            "    ",
            "    fn len(&self) -> usize {",
            "        self.data.len()",
            "    }",
            "    ",
            "    fn resize(&mut self, new_len: usize, val: u8) {",
            "        if new_len > self.len() {",
            "            self.data.resize(new_len, val);",
            "        } else {",
            "            self.data.truncate(new_len);",
            "        }",
            "    }",
            "    ",
            "    fn put_bytes(&mut self, val: u8, cnt: usize) {",
            "        let new_len = self.len().saturating_add(cnt);",
            "        self.resize(new_len, val);",
            "    }",
            "}"
