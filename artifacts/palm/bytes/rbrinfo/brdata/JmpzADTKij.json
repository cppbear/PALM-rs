    "pub(crate) fn from_vec(vec: Vec<u8>) -> BytesMut {",
    "    let mut vec = ManuallyDrop::new(vec);",
    "    let ptr = vptr(vec.as_mut_ptr());",
    "    let len = vec.len();",
    "    let cap = vec.capacity();",
    "",
