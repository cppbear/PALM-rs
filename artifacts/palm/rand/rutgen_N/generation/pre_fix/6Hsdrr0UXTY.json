                  "        pub fn read_u64_into(src: &[u8], dst: &mut [u64]) {",
                  "            for (i, chunk) in src.chunks(8).enumerate() {",
                  "                dst[i] = chunk.iter().enumerate().fold(0u64, |acc, (j, &b)| {",
                  "                    acc | ((b as u64) << (j * 8))",
                  "                });",
                  "            }",
                  "        }",
                  "    }",
