            "#[derive(Default)]",
            "struct TokenStream {",
            "    tokens: Vec<String>,",
            "}",
            "",
            "trait ToTokens {",
            "    fn to_tokens(&self, stream: &mut TokenStream);",
            "}",
            "",
            "impl ToTokens for String {",
            "    fn to_tokens(&self, stream: &mut TokenStream) {",
            "        stream.tokens.push(self.clone());",
            "    }",
            "}",
            "",
