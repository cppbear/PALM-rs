            "#[derive(Default)]",
            "struct TestWriter {",
            "    bytes: Vec<u8>,",
            "    offset: usize,",
            "}",
            "",
            "impl TestWriter {",
            "    fn new(size: usize) -> Self {",
            "        TestWriter {",
            "            bytes: vec![0; size],",
            "            offset: 0,",
            "        }",
            "    }",
            "",
            "    fn write_str(&mut self, s: &str) -> std::fmt::Result {",
            "        if self.offset + s.len() > self.bytes.len() {",
            "            Err(std::fmt::Error)",
            "        } else {",
            "            self.bytes[self.offset..self.offset + s.len()].copy_from_slice(s.as_bytes());",
            "            self.offset += s.len();",
            "            Ok(())",
            "        }",
            "    }",
            "}"
