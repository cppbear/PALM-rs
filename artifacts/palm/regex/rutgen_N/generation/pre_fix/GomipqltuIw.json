            "#[derive(Debug)]",
            "struct Literal {",
            "    value: String,",
            "}",
            "",
            "impl Literal {",
            "    fn new(value: &str) -> Self {",
            "        Self {",
            "            value: value.to_string(),",
            "        }",
            "    }",
            "",
            "    fn reverse(&mut self) {",
            "        self.value = self.value.chars().rev().collect();",
            "    }",
            "}",
            "",
            "#[derive(Debug)]",
            "struct LiteralCollection {",
