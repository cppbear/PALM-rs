            "    fn new(lits: Vec<Literal>) -> Self {",
            "        Self { lits }",
            "    }",
            "",
            "    pub fn reverse(&mut self) {",
            "        for lit in &mut self.lits {",
            "            lit.reverse();",
            "        }",
            "    }",
            "}"
