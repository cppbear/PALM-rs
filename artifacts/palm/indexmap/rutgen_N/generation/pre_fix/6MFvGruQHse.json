            "struct TestSet<T> {",
            "    elements: Vec<T>,",
            "}",
            "",
            "impl<T: PartialEq> TestSet<T> {",
            "    pub fn replace(&mut self, value: T) -> Option<T> {",
            "        if let Some(pos) = self.elements.iter().position(|x| *x == value) {",
            "            Some(std::mem::replace(&mut self.elements[pos], value))",
            "        } else {",
            "            self.elements.push(value);",
            "            None",
            "        }",
            "    }",
            "}"
