                  "    // Test case: No match, but not at the end of the text",
                  "    assert!(!regex.is_match_at(b\"Beta\", 0));",
                  "    ",
                  "    // Test case: Match at non-zero start position",
                  "    assert!(regex.is_match_at(b\"Beta Alpha\", 6));",
                  "    ",
