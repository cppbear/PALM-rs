            "#[derive(Clone, PartialEq)]",
            "enum Flag {",
            "    IgnoreCase,",
            "    Multiline,",
            "}",
            "",
            "enum FlagsItemKind {",
            "    Negation,",
            "    Flag(Flag),",
            "}",
            "",
            "struct FlagsItem {",
            "    kind: FlagsItemKind,",
            "}",
            "",
            "struct FlagsSet {",
            "    items: Vec<FlagsItem>,",
            "}",
            "",
            "impl FlagsSet {",
            "    pub fn new(items: Vec<FlagsItem>) -> Self {",
            "        Self { items }",
            "    }",
            "",
            "    pub fn flag_state(&self, flag: Flag) -> Option<bool> {",
            "        let mut negated = false;",
            "        for x in &self.items {",
            "            match x.kind {",
            "                FlagsItemKind::Negation => {",
            "                    negated = true;",
            "                }",
            "                FlagsItemKind::Flag(ref xflag) if xflag == &flag => {",
            "                    return Some(!negated);",
            "                }",
            "                _ => {}",
            "            }",
            "        }",
            "        None",
            "    }",
            "}"
          ],
