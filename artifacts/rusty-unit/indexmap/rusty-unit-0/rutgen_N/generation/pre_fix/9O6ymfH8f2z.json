            "#[derive(Debug)]",
            "struct Keys<'a, K, V> {",
            "    entries: &'a [(K, V)],",
            "    index: usize,",
            "}",
            "",
            "impl<'a, K, V> Keys<'a, K, V> {",
            "    fn new(entries: &'a [(K, V)]) -> Self {",
            "        Keys { entries, index: 0 }",
            "    }",
            "}",
            "",
            "impl<'a, K, V> Iterator for Keys<'a, K, V> ",
            "where ",
            "    K: Copy,",
            "{",
            "    type Item = K;",
            "",
            "    fn next(&mut self) -> Option<Self::Item> {",
            "        if self.index < self.entries.len() {",
            "            let key = self.entries[self.index].0;",
            "            self.index += 1;",
            "            Some(key)",
            "        } else {",
            "            None",
            "        }",
            "    }",
            "}",
            "",
            "struct MapSlice<K, V> {",
            "    entries: Vec<(K, V)>,",
            "}",
            "",
            "impl<K, V> MapSlice<K, V> {",
            "    fn new(entries: Vec<(K, V)>) -> Self {",
            "        MapSlice { entries }",
            "    }",
            "",
            "    pub fn keys(&self) -> Keys<'_, K, V> {",
            "        Keys::new(&self.entries)",
            "    }",
            "}"
