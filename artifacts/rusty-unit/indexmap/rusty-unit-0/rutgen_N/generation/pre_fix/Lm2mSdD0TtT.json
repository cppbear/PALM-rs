            "impl<K, V> MyMap<K, V> {",
            "    fn new() -> Self {",
            "        MyMap { data: Vec::new() }",
            "    }",
            "",
            "    pub(crate) fn shift_remove_index(&mut self, index: usize) -> Option<(K, V)> {",
            "        if index >= self.data.len() {",
            "            return None;",
            "        }",
            "        let entry = self.data.remove(index);",
            "        Some(entry)",
            "    }",
            "",
            "    fn insert(&mut self, key: K, value: V) {",
            "        self.data.push((key, value));",
            "    }",
            "}"
