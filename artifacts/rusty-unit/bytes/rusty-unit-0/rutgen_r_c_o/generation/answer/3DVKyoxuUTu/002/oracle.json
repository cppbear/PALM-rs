        "prefix": [
          "{",
          "    let mut bytes = Bytes::from_static(b\"test\");",
          "    let cnt = bytes.len() + 1; // cnt is one more than length",
          "    bytes.advance(cnt);",
          "}"
        ],
        "oracles": [
          [
            "    let bytes = Bytes::from_static(b\"test\");",
            "    let cnt = bytes.len() + 1;",
            "    assert!(cnt > bytes.len());"
          ],
          [
            "    let bytes = Bytes::from_static(b\"test\");",
            "    let cnt = bytes.len() + 1;",
            "    assert_panics!();"
          ]
        ],
